  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@shipsite.dev/components": "^0.1.0"
  }
